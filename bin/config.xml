<?xml version='1.0' encoding='ASCII'?>
<!DOCTYPE configuration [
<!ENTITY STDELMB_FOUNDATIONS SYSTEM "CANopen_def_STDELMB_FOUNDATIONS.xmle">
<!ENTITY STDELMB_DI_TPDO1_C SYSTEM "CANopen_def_STDELMB_DI_TPDO1_C.xmle">
