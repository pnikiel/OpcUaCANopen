		<Node name="elmb2" requestedState="OPERATIONAL" id="2" stateInfoSource="NodeGuard">
			&STDELMB_FOUNDATIONS;
			&STDELMB_DI_TPDO1_C;
      			&STDELMB_AI_TPDO3;
			<SdoVariable index="1234" subIndex="1" name="NONEXISTENT" dataType="Byte" access="R"/>
